r\n                            <button className=\"view-specialists-btn\" style={{ background: category.color }}>\r\n                                View Specialists ΓåÆ\r\n                            </button>\r\n                        </div>\r\n                    ))}\r\n                </div>\r\n            ) : (\r\n                <div className=\"specialists-view\">\r\n                    {/* Back Button */}\r\n                    <button className=\"back-btn\" onClick={() => setSelectedCategory(null)}>\r\n                        ΓåÉ Back to Categories\r\n                    </button>\r\n\r\n                    {/* Category Header */}\r\n                    <div className=\"selected-category-header\">\r\n                        <div className=\"category-info\">\r\n                            <span className=\"category-icon-large\" style={{ background: categories[selectedCategory].color }}>\r\n                                {categories[selectedCategory].icon}\r\n                            </span>\r\n                            <div>\r\n                                <h2>{categories[selectedCategory].name}</h2>\r\n                                <p>{categories[selectedCategory].description}</p>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div className=\"category-stats-grid\">\r\n                            <div className=\"stat-box\">\r\n                                <span className=\"stat-number\">{categoryStats.activeSpecialists}</span>\r\n                                <span className=\"stat-label\">Active Specialists</span>\r\n                            </div>\r\n                            <div className=\"stat-box\">\r\n                                <span className=\"stat-number\">{categoryStats.totalJobs}</span>\r\n                                <span className=\"stat-label\">Jobs Completed</span>\r\n                            </div>\r\n                            <div className=\"stat-box\">\r\n                                <span className=\"stat-number\">{categoryStats.avgCompletionTime}</span>\r\n                                <span className=\"stat-label\">Avg Completion</span>\r\n                            </div>\r\n                            <div className=\"stat-box\">\r\n                                <span className=\"stat-number\">{categoryStats.successRate}%</span>\r\n                                <span className=\"stat-label\">Success Rate</span>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    {/* Filters */}\r\n                    <div className=\"filters\">\r\n                        <div className=\"filter-group\">\r\n                            <label>Proficiency Level:</label>\r\n                            <select value={filterLevel} onChange={(e) => setFilterLevel(e.target.value)}>\r\n                                <option value=\"all\">All Levels</option>\r\n                                {proficiencyLevels.map(level => (\r\n                                    <option key={level.id} value={level.id}>\r\n                                        {level.badge} {level.name}\r\n                                    </option>\r\n                                ))}\r\n                            </select>\r\n                        </div>\r\n\r\n                        <div className=\"filter-group\">\r\n                            <label>Sort By:</label>\r\n                            <select value={sortBy} onChange={(e) => setSortBy(e.target.value)}>\r\n                                <option value=\"rating\">Highest Rating</option>\r\n                                <option value=\"projects\">Most Projects</option>\r\n                                <option value=\"earnings\">Top Earners</option>\r\n                                <option value=\"rate\">Lowest Rate</option>\r\n                            </select>\r\n                        </div>\r\n                    </div>\r\n\r\n                    {/* Specialists List */}\r\n                    <div className=\"specialists-list\">\r\n                        {filteredAndSortedSpecialists.map((specialist, index) => (\r\n                            <div key={index} className=\"specialist-card\">\r\n                                <div className=\"specialist-header\">\r\n                                    <div className=\"specialist-avatar\">{specialist.avatar}</div>\r\n                                    <div className=\"specialist-info\">\r\n                                        <h3>{specialist.name}</h3>\r\n                                        <p className=\"specialist-address\">{specialist.address}</p>\r\n                                        <div className=\"proficiency-badge\" style={{ background: proficiencyLevels[specialist.proficiency].color }}>\r\n                                            {proficiencyLevels[specialist.proficiency].badge} {proficiencyLevels[specialist.proficiency].name}\r\n                                        </div>\r\n                                    </div>\r\n                                    <div className=\"specialist-rate\">\r\n                                        <span className=\"rate-amount\">${specialist.hourlyRate}</span>\r\n                                        <span className=\"rate-label\">/hour</span>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <div className=\"specialist-stats\">\r\n                                    <div className=\"stat-item\">\r\n                                        <span className=\"stat-icon\">Γ¡É</span>\r\n                                        <span className=\"stat-text\">{specialist.averageRating}% Rating</span>\r\n                                    </div>\r\n                                    <div className=\"stat-item\">\r\n                                        <span className=\"stat-icon\">Γ£à</span>\r\n                                        <span className=\"stat-text\">{specialist.verifiedProjects} Projects</span>\r\n                                    </div>\r\n                                    <div className=\"stat-item\">\r\n                                        <span className=\"stat-icon\">≡ƒÆ░</span>\r\n                                        <span className=\"stat-text\">${(specialist.totalEarnings / 1000).toFixed(0)}k Earned</span>\r\n                                    </div>\r\n                                    <div className=\"stat-item\">\r\n                                        <span className=\"stat-icon\">≡ƒæÑ</span>\r\n                                        <span className=\"stat-text\">{specialist.endorsements} Endorsements</span>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <div className=\"specialist-specialties\">\r\n                                    {specialist.specialties.map((specialty, i) => (\r\n                                        <span key={i} className=\"specialty-tag\">{specialty}</span>\r\n                                    ))}\r\n                                </div>\r\n\r\n                                <div className=\"specialist-availability\">\r\n                                    <span className={`availability-badge ${specialist.availability.toLowerCase()}`}>\r\n                                        {specialist.availability === 'Available' ? '≡ƒƒó' : '≡ƒƒí'} {specialist.availability}\r\n                                    </span>\r\n                                    <span className=\"response-time\">\r\n                                        ΓÜí Responds {specialist.responseTime}\r\n                                    </span>\r\n                                </div>\r\n\r\n                                <div className=\"specialist-actions\">\r\n                                    <button\r\n                                        className=\"hire-btn\"\r\n                                        onClick={() => handleHireSpecialist(specialist)}\r\n                                    >\r\n                                        ≡ƒÆ╝ Hire Now\r\n                                    </button>\r\n                                    <button\r\n                                        className=\"endorse-btn\"\r\n                                        onClick={() => handleEndorse(specialist)}\r\n                                    >\r\n                                        ≡ƒæì Endorse\r\n                                    </button>\r\n                                    <button className=\"portfolio-btn\">\r\n                                        ≡ƒôü Portfolio\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                        ))}\r\n                    </div>\r\n                </div>\r\n            )}\r\n\r\n            {/* Become a Specialist CTA */}\r\n            <div className=\"specialist-cta\">\r\n                <h2>Are you a Web3 specialist?</h2>\r\n                <p>Join our elite network and get matched with high-value projects</p>\r\n                <button className=\"register-specialist-btn\">\r\n                    ≡ƒÜÇ Register as Specialist\r\n                </button>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default SpecialistMarketplace;\r\n","usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\StripeOnrampModal.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\TermsOfService.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\TokenizeAssetForm.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\UserLink.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\WithdrawButton.jsx","messages":[{"ruleId":"no-unused-vars","severity":1,"message":"'formatEther' is defined but never used. Allowed unused vars must match /^[A-Z_]/u.","line":6,"column":10,"nodeType":"Identifier","messageId":"unusedVar","endLine":6,"endColumn":21}],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"import React, { useState } from 'react';\r\nimport { useWriteContract, useReadContract, usePublicClient } from 'wagmi';\r\nimport { Coins, Loader2, RefreshCcw } from 'lucide-react';\r\nimport FreelanceEscrowABI from '../contracts/FreelanceEscrow.json';\r\nimport { CONTRACT_ADDRESS, SUPPORTED_TOKENS } from '../constants';\r\nimport { formatEther, formatUnits } from 'viem';\r\nimport { showPendingToast, updateToastToSuccess, updateToastToError, handleError } from '../utils/feedback';\r\n\r\nexport default function WithdrawButton({ address }) {\r\n    const client = usePublicClient();\r\n    const [selectedToken, setSelectedToken] = useState(SUPPORTED_TOKENS[0].address);\r\n    const [isConfirming, setIsConfirming] = useState(false);\r\n\r\n    const { data: balanceData, refetch, isLoading: isReading } = useReadContract({\r\n        address: CONTRACT_ADDRESS,\r\n        abi: FreelanceEscrowABI.abi,\r\n        functionName: 'balances',\r\n        args: [address, selectedToken],\r\n    });\r\n\r\n    const { writeContractAsync, isPending } = useWriteContract();\r\n\r\n    const handleWithdraw = async () => {\r\n        try {\r\n            const hash = await writeContractAsync({\r\n                address: CONTRACT_ADDRESS,\r\n                abi: FreelanceEscrowABI.abi,\r\n                functionName: 'withdraw',\r\n                args: [selectedToken],\r\n            });\r\n            const toastId = showPendingToast(hash);\r\n            setIsConfirming(true);\r\n            const receipt = await client.waitForTransactionReceipt({ hash });\r\n            setIsConfirming(false);\r\n            if (receipt.status === 'success') {\r\n                updateToastToSuccess(toastId, \"Withdrawal Successful!\");\r\n                refetch();\r\n            } else {\r\n                updateToastToError(toastId, { shortMessage: \"Transaction Reverted\" });\r\n            }\r\n        } catch (error) {\r\n            setIsConfirming(false);\r\n            handleError(error);\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            <div style={{ display: 'flex', alignItems: 'center', gap: 8, marginBottom: 16 }}>\r\n                <Coins size={16} style={{ color: 'var(--accent-light)' }} />\r\n                <h3 style={{ fontSize: '0.82rem', fontWeight: 700 }}>Withdrawable Balance</h3>\r\n            </div>\r\n\r\n            <div style={{ display: 'flex', gap: 6, marginBottom: 16 }}>\r\n                {SUPPORTED_TOKENS.map(token => (\r\n                    <button\r\n                        key={token.symbol}\r\n                        onClick={() => setSelectedToken(token.address)}\r\n                        style={{\r\n                            fontSize: '0.72rem', fontWeight: 700, padding: '5px 12px',\r\n                            borderRadius: 8, cursor: 'pointer', transition: 'all 0.15s ease',\r\n                            background: selectedToken === token.address ? 'rgba(124,92,252,0.12)' : 'rgba(255,255,255,0.03)',\r\n                            border: `1px solid ${selectedToken === token.address ? 'rgba(124,92,252,0.3)' : 'var(--border)'}`,\r\n                            color: selectedToken === token.address ? 'var(--accent-light)' : 'var(--text-tertiary)',\r\n                        }}\r\n                    >\r\n                        {token.symbol}\r\n                    </button>\r\n                ))}\r\n            </div>\r\n\r\n            <div style={{ display: 'flex', alignItems: 'flex-end', justifyContent: 'space-between' }}>\r\n                <div>\r\n                    <div style={{ fontSize: '1.6rem', fontWeight: 800, letterSpacing: '-0.02em', display: 'flex', alignItems: 'center', gap: 8 }}>\r\n                        {balanceData ? formatUnits(balanceData, SUPPORTED_TOKENS.find(t => t.address === selectedToken)?.decimals || 18) : '0.0'}\r\n                        {isReading && <Loader2 size={14} style={{ opacity: 0.4, animation: 'spin 1s linear infinite' }} />}\r\n                    </div>\r\n                    <span style={{ fontSize: '0.72rem', color: 'var(--text-tertiary)' }}>Available to claim</span>\r\n                </div>\r\n\r\n                <button\r\n                    onClick={handleWithdraw}\r\n                    disabled={!balanceData || balanceData === 0n || isPending || isConfirming}\r\n                    className=\"btn btn-primary btn-sm\"\r\n                    style={{ borderRadius: 10, display: 'flex', alignItems: 'center', gap: 6, opacity: (!balanceData || balanceData === 0n || isPending || isConfirming) ? 0.5 : 1 }}\r\n                >\r\n                    {isPending || isConfirming ? (\r\n                        <><Loader2 size={13} style={{ animation: 'spin 1s linear infinite' }} /> Processing</>\r\n                    ) : (\r\n                        <><RefreshCcw size={13} /> Withdraw</>\r\n                    )}\r\n                </button>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n","usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\components\\YieldManagerDashboard.jsx","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\config\\env.js","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\constants.js","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\hooks\\useAnimeAnimations.js","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\hooks\\useArbitration.js","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\akhil\\.gemini\\antigravity\\scratch\\polygon-freelance-marketplace\\frontend\\src\\hooks\\useEthersSigner.js","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableW